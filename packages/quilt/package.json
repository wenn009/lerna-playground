{
  "name": "@foursquare/quilt",
  "version": "0.1.0",
  "description": "React Components that implement Foursquare's Design",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/foursquare/web-packages.git",
  "author": "Steven Lam <Stevo14850@gmail.com>",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://foursquarfdedev.jfrog.io/foursquaredev/api/npm/npm/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:icons": "rimraf ./src/components/Icon/icons && svgr --config-file .svgrrc.js --ext 'tsx' --template .svgr-tsx-template.js --out-dir ./src/components/Icon/icons ./assets/svg",
    "build:ts": "npm run ts",
    "dev": "npm run && npm run dev:ts & sleep 45 && npm run dev:storybook",
    "dev:storybook": "npm run storybook:dev",
    "dev:ts": "npm run ts:watch",
    "lint": "tslint -p ./tsconfig.json --fix && tslint 'src/stories/**/*.ts?(x)' --fix",
    "prettier:list": "prettier --list-different \"{src/**,docs/**,./}/*.{ts,tsx,js,json,css,md}\"",
    "prettier:write": "prettier --write \"{src/**,docs/**,./}/*.{ts,tsx,js,json,css,md}\"",
    "storybook:deploy": "storybook-to-aws-s3 --bucket-path=foursquare-cupcake",
    "storybook:dev": "start-storybook -p 9001 -c .storybook --ci",
    "test": "is-ci && npm run test:coverage || npm run test:watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "ts": "rimraf ./dist && npm run build:icons && tsc -p tsconfig.json",
    "ts:watch": "npm run ts -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "peerDependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.2.6",
    "@storybook/source-loader": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@storybook/theming": "^5.2.6",
    "@svgr/cli": "^4.2.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.116",
    "@types/node": "^11.13.0",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/storybook__addon-info": "^3.4.1",
    "@types/styled-components": "^4.1.16",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "css-loader": "^3.2.0",
    "husky": "^1.1.2",
    "is-ci": "^2.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rimraf": "^2.6.2",
    "style-loader": "^1.0.0",
    "styled-components": "^4.1.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^6.0.0",
    "tslint": "^5.11.0",
    "typescript": "3.5.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "is-url": "^1.2.4",
    "react-loadable": "^5.5.0"
  }
}
