{
  "name": "@foursquare/quilt",
  "version": "0.0.0",
  "description": "React Components that implement Foursquare's Design",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/foursquare/web-packages.git",
  "author": "Steven Lam <Stevo14850@gmail.com>",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://foursquaredev.jfrog.io/foursquaredev/api/npm/npm/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:storybook",
    "build-and-serve": "npm run build && npm run serve",
    "build:icons": "rimraf ./src/components/Icon/icons && svgr --config-file .svgrrc.js --ext 'tsx' --template .svgr-tsx-template.js --out-dir ./src/components/Icon/icons ./assets/svg",
    "build:storybook": "npm run storybook:build",
    "build:ts": "npm run ts && npm run copy:styles.css",
    "copy:styles.css": "copyfiles -f ./src/themes/default/styles.css ./dist/themes/default",
    "create:component": "npm run print:component-instructions && hygen component new",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "dev": "npm run && npm run dev:ts & sleep 15 && npm run dev:documentation",
    "dev:storybook": "npm run storybook:dev",
    "dev:ts": "npm run ts:watch",
    "lint": "tslint -p ./tsconfig.json --fix && tslint 'src/stories/**/*.ts?(x)' --fix",
    "prepack": "npm run build:ts",
    "pretest:e2e:run": "npm run build:documentation",
    "prettier:list": "prettier --list-different \"{src/**,docs/**,./}/*.{ts,tsx,js,json,css,md}\"",
    "prettier:write": "prettier --write \"{src/**,docs/**,./}/*.{ts,tsx,js,json,css,md}\"",
    "print:component-instructions": "node ./scripts/componentInstructions.js",
    "serve": "http-server ./static -p 8080",
    "storybook:build": "build-storybook -c .storybook -o ./static/storybook",
    "storybook:build:watch": "npm run storybook:build -- -w",
    "storybook:dev": "start-storybook -p 9001 -c .storybook --ci",
    "test": "is-ci && npm run test:coverage || npm run test:watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "is-ci && npm run test:e2e:run || npm run test:e2e:dev",
    "test:e2e:run": "start-server-and-test build-and-serve http://localhost:8080/ cy:run",
    "test:e2e:dev": "start-server-and-test serve http://localhost:8080/ cy:open",
    "test:watch": "jest --watchAll",
    "ts": "rimraf ./dist && npm run build:icons && tsc -p tsconfig.json",
    "ts:watch": "npm run ts -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "peerDependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.2.6",
    "@storybook/source-loader": "^5.3.19",
    "@storybook/theming": "^5.2.6",
    "@svgr/cli": "^4.2.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.116",
    "@types/node": "^11.13.0",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/storybook__addon-info": "^3.4.1",
    "@types/styled-components": "^4.1.16",
    "antd": "^3.23.2",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^3.2.0",
    "cypress": "^3.1.4",
    "cypress-testing-library": "^2.3.5",
    "faker": "^4.1.0",
    "http-server": "^0.11.1",
    "husky": "^1.1.2",
    "hygen": "^2.0.4",
    "is-ci": "^2.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "lint-staged": "^7.0.5",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.10.2",
    "react-docgen": "^4.1.0",
    "react-dom": "^16.10.2",
    "react-testing-library": "^5.4.4",
    "recast": "^0.17.5",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^1.0.0",
    "styled-components": "^4.1.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^6.0.0",
    "tslint": "^5.11.0",
    "typescript": "3.5.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@tippy.js/react": "^3.1.0",
    "@types/react-table": "^6.8.5",
    "is-url": "^1.2.4",
    "numeral": "^2.0.6",
    "react-loadable": "^5.5.0",
    "react-table": "^6.10.0",
    "uuid": "^3.3.2",
    "warning": "^4.0.3"
  }
}
